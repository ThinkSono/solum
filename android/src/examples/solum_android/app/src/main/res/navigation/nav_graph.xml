<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="me.clarius.sdk.solum.example.FirstFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_first">

        <action
            android:id="@+id/action_FirstFragment_to_BluetoothFragment"
            app:destination="@id/BluetoothFragment" />
        <action
            android:id="@+id/action_FirstFragment_to_wifiFragment"
            app:destination="@id/wifiFragment" />
    </fragment>
    <fragment
        android:id="@+id/BluetoothFragment"
        android:name="me.clarius.sdk.solum.example.BluetoothFragment"
        android:label="@string/bluetooth_fragment_label"
        tools:layout="@layout/fragment_bluetooth">

        <action
            android:id="@+id/action_BluetoothFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>
    <fragment
        android:id="@+id/wifiFragment"
        android:name="me.clarius.sdk.solum.example.WifiFragment"
        android:label="@string/wifi_fragment_label"
        tools:layout="@layout/fragment_wifi">
        <action
            android:id="@+id/action_wifiFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>
</navigation>
